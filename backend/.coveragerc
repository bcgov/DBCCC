[run]
branch = true
include = backend/*
omit =
    *migrations*
    *tests*
    manage.py
    *wsgi.py
    *asgi.py

[report]
show_missing = true
exclude_lines =
    pragma: no cover
    pass
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
